{
  "name": "code",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios12": "react-native run-ios --simulator='iPhone 12 Pro'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && pod install && cd ..",
    "clean": "npm run clean-android && npm run clean-ios",
    "android-release": "react-native run-android --variant=release",
    "fast": "npm install && npm run clean && npm run android && npm run ios12",
    "fast-release": "npm install && npm run clean-android && npm run android-release"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "react": "17.0.2",
    "react-native": "^0.66.4",
    "react-native-camera": "^4.2.1",
    "react-native-image-base64": "^0.1.4",
    "react-native-image-crop-picker": "^0.38.1",
    "react-native-image-helper": "^0.0.3",
    "react-native-image-picker": "^5.6.1",
    "react-native-image-resizer": "^1.4.5",
    "react-native-orientation-locker": "^1.1.0",
    "react-native-permissions": "^3.8.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.23.0",
    "react-native-vision-camera": "^2.16.5"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/runtime": "7.19.0",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "26.6.3",
    "eslint": "^7.14.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
